{
    "manifest_version": 3,
    "name": "Attendance Check",
    "version": "1.0",
    "description": "Extension để điểm danh theo danh sách CSV.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "web_accessible_resources": [
      {
        "resources": ["script/*.js", "style/*.css", "images/*.*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  